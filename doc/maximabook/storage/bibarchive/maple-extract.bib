@Article{Kouchakpour:1994:SCR,
  author =       "Peyman Kouchakpour and Albert Y. Zomaya",
  title =        "Symbolic computation of robot dynamic parameters",
  journal =      j-CYBERNETICS-SYSTEMS,
  volume =       "25",
  number =       "5",
  pages =        "697--727",
