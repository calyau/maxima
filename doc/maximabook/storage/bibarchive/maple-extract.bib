@Article{vanHoeij:1994:ACI,
  author =       "Mark {van Hoeij}",
  title =        "An algorithm for computing an integral basis in an
                 algebraic function field",
  journal =      j-J-SYMBOLIC-COMP,
  volume =       "18",
