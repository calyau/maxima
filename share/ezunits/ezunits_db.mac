/* A small database of elementary unit conversions */

/* Undefined and therefore treated as basic units:
 *
 * m kg s A K mol
 *
 */

declare_unit_conversion
   (Hz = 1/s,           /* hertz:   Frequency            */
    hertz = Hz,
    N = kg*m/s^2,       /* newton:  Force                */
    newton = N,
    Pa = N/m^2,         /* pascal:  Pressure             */
    pascal = Pa,
    J = N*m,            /* joule:   Work, energy         */
    joule = J,
    W = J/s,            /* watt:    Power                */
    watt = W,
    C = s*A,            /* coulomb: Electric charge      */
    coulomb = C,
    V = J/C,            /* volt:    Electromotive force  */
    volt = V,
    F = C^2/J,          /* farad:   Electric capacitance */
    farad = F,
    Ohm = J*s/C^2,      /* ohm:     Electric resistance  */
    ohm = Ohm,
    S = 1/Ohm,          /* siemens: Electric conductance */
    siemens = S,
    Wb = J/A,           /* weber:   Magnetic flux        */
    weber = Wb,
    T = J/m^2/A,        /* tesla:   Magnetic flux density*/
    tesla = T,
    H = J/A^2,          /* henry:   Inductance           */
    henry = H,

    fm = m/10^15,
    pm = m/10^12,
    nm = m/10^9,
    microm = m/10^6,
    micron = microm,
    mm = m/1000,
    km = 1000*m,
    Mm = 1000000*m,
    Gm = 1000000000*m,

    fg = g/10^15,
    pg = g/10^12,
    ng = g/10^9,
    microg = g/10^6,
    mg = kg/1000000,
    g = kg/1000,
    Mg = 1000*kg,
    Gg = 1000000*kg,

    fs = s/10^15,
    ps = s/10^12,
    ns = s/10^9,
    micros = s/10^6,
    ms = s/1000,
    ks = 1000*s,
    Ms = 1000000*s,
    Gs = 1000000000*s,

    fA = A/10^15,
    pA = A/10^12,
    nA = A/10^9,
    microA = A/10^6,
    microampere = microA,
    mA = A/1000,
    ampere = A,
