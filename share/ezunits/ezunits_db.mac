declare_unit_conversion
   (feet = 3048/10000 * m,
    /* International pound avoirdupois */
    pound_mass = 45359237/10^8 * kg,
    minute = 60*s,
    hour = 3600*s,
    day = 86400*s,
    week = 7*86400*s,
    month = (30 + 7/16)*86400*s,
    year = (365 + 1/4)*86400*s,
    Btu = 1055*J,
    R = 5/9*K);
   
declare_unit_conversion
   (inch = feet/12,
    yard = 3*feet,
    mile = 5280*feet,
    gallon = 231*inch^3,
    slug = pound_force*s^2/foot,
    ounce = pound_mass/16,
    /* grain = ???, */
    /* assume gravitational acceleration = 980.665 cm/s^2 */
    pound_force = pound_mass*196133/6096*ft/s^2,
    acre = 55*88*yard^2,
    psi = pound_force/inch^2,
    cfm = feet^3/minute,

    ft = feet,
    foot = feet,
    lbm = pound_mass,
    oz = ounce,
    lbf = pound_force,
    horsepower = 550 * pound_force * foot / s,
    hp = horsepower,

    short_ton = 2000*lbm,
    rod = 33/2*feet,

    quart = gallon / 4,
    pint = quart / 2,
    cup = pint / 2,
    gill = cup / 2,
    fluid_ounce = cup / 8,
    fl_oz = fluid_ounce,
    tablespoon = fluid_ounce / 2,
    teaspoon = tablespoon / 3,
    tbsp = tablespoon,
    tsp = teaspoon);
