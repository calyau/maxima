    kmol = 1000*mol,
    Gmol = 1000000*mol,
    Mmol = 1000000000*mol,

    fHz = Hz/10^15,
    pHz = Hz/10^12,
    nHz = Hz/10^9,
    microHz = Hz/10^6,
    microhertz = microHz,
    mHz = Hz/1000,
    kHz = 1000*Hz,
    MHz = 1000000*Hz,
    GHz = 1000000000*Hz,

    fN = N/10^15,
    pN = N/10^12,
    nN = N/10^9,
    microN = N/10^6,
    micronewton = microN,
    mN = N/1000,
    kN = 1000*N,
    MN = 1000000*N,
    GN = 1000000000*N,

    fPa = Pa/10^15,
    pPa = Pa/10^12,
    nPa = Pa/10^9,
    microPa = Pa/10^6,
    micropascal = microPa,
    mPa = Pa/1000,
    kPa = 1000*Pa,
    MPa = 1000000*Pa,
    GPa = 1000000000*Pa,

    fJ = J/10^15,
    pJ = J/10^12,
    nJ = J/10^9,
    microJ = J/10^6,
    microjoule = microJ,
    mJ = J/1000,
    kJ = 1000*J,
    MJ = 1000000*J,
    GJ = 1000000000*J,

    fW = W/10^15,
    pW = W/10^12,
    nW = W/10^9,
    microW = W/10^6,
    microwatt = microW,
    mW = W/1000,
    kW = 1000*W,
    MW = 1000000*W,
    GW = 1000000000*W,

    fC = C/10^15,
    pC = C/10^12,
    nC = C/10^9,
    microC = C/10^6,
